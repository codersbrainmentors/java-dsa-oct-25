class Solution {
    public boolean isIsomorphic(String s, String t) {
        if(s.length() != t.length()){
            return false;
        }
        // char m1 [] = new char[256]; // ascii
        // char m2 [] = new char[256]; // ascii
        HashMap<Character, Character> m1 = new HashMap<>();
        HashMap<Character, Character> m2 = new HashMap<>();
        for(int i = 0 ; i<s.length(); i++){
            char a = s.charAt(i);
            char b = t.charAt(i);
            if(m1.containsKey(a)){
                if(m1.get(a) != b){
                    return false;
                }
               
            }
             else{
                    m1.put(a,b);
                }
             if(m2.containsKey(b)){
                if(m2.get(b)!=a){
                    return false;
                }
               
            }
             else{
                    m2.put(b,a);
                }
           
        }
     return true;

    }
}
